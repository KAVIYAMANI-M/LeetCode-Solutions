class Solution {
    public int GCD(int a,int b)
    {
       if(b==0)
       {
        return a;
       }
       return GCD(b,a%b);
    }
    public int LCM(int a,int b)
    {
        int gcd=GCD(a,b);
        return a*b/gcd;
    }
    public int subarrayLCM(int[] nums, int k) {
        int count=0;
        for(int i=0;i<nums.length;i++)
        {
            int currentLCM=1;
            for(int j=i;j<nums.length;j++)
            {
                currentLCM=LCM(currentLCM,nums[j]);
                if(currentLCM==k)
                {
                    count++;
                }
                if(currentLCM > k) break;
            }
        }
        return count;
    }
}
